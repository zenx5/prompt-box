{
    "name": "Prompt Box",
    "description": "Almacena tus prompts para IA",
    "version": "2",
    "icons":{
        "16":"logo.png",
        "32":"logo.png",
        "64":"logo.png",
        "128":"logo.png"
    },
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "options_page":"index.html?path=configuration",
    "permissions": ["storage"],
    "action": {
        "default_popup": "index.html?path=popup"
    }
  }