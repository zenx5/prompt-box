{
    "name": "Prompt Box",
    "description": "Almacena tus prompts para IA",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "src/js/background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "src/popup.html"
    }
  }